{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=IdentityJwtDatabase.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "Issuer": "IdentityJWT",
    "Audience": "ApiUser",
    "SecretKey": "<complex hard-to-guess string (I used a very long password generated in LastPass)>",
    "TokenExpireSeconds":  "300"
  }
}
